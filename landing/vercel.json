{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/css/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "redirects": [
    { "source": "/github", "destination": "https://github.com/nicepkg/pilos-agents", "permanent": false },
    { "source": "/discord", "destination": "https://discord.gg/PLACEHOLDER", "permanent": false },
    { "source": "/releases", "destination": "https://github.com/nicepkg/pilos-agents/releases", "permanent": false }
  ]
}
